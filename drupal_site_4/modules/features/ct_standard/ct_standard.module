<?php
/**
 * @file
 * Code for the Standard Content Type feature.
 */

include_once 'ct_standard.features.inc';


/**
 * Implements hook_ctools_plugin_directory().
 */
function ct_standard_ctools_plugin_directory($owner, $plugin_type) {
  if ($owner == 'ctools' && $plugin_type == 'content_types') {
    return 'plugins/content_types';
  }
}
